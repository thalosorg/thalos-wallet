{
  "name": "ThalosWallet",
  "productName": "thalos-wallet",
  "version": "1.0.0.0",
  "description": "Thalos Desktop Wallet. www.thalos.org",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "rebuild": ".\\node_modules\\.bin\\electron-rebuild.cmd -v 2.0.2",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "Thalos.org",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/build/thalos_icon.ico",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "Thalos-Wallet"
      },
      "electronInstallerDebian": {
        "icon": "/src/build/thalos_icon.ico",
        "categories": [
          "Utility"
        ],
        "homepage": "https://thalos.org"
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "Thalos.Wallet",
        "name": "Thalos-Wallet"
      }
    }
  },
  "dependencies": {
    "blade": "^3.3.1",
    "body-parser": "^1.18.3",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "log4js": "^2.8.0",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt": "^1.4.13",
    "electron-prebuilt-compile": "2.0.2",
    "electron-rebuild": "^1.7.3",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.1"
  }
}
